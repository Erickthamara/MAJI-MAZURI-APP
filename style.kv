ScreenManager:
    MyScreen:
    OrderScreen:
    CatalogueScreen:
    SalesScreen:
    

<MyScreen>
    name:"test"


<OrderScreen>
    name:"orders"

    
    BoxLayout:
        orientation: "vertical"

        MDTopAppBar:
            title: "MDTopAppBar"
            right_action_items: [["dots-vertical", lambda x: app.callback()]]
        
        MDFloatLayout:
            id:my_float_layout
            MDRaisedButton:
                size_hint: .5,.01
                id:delete_row
                disabled:False
                text:"Delete Row"
                theme_text_color:"Custom"
                text_color: 0,0,0,1
                pos_hint:{'center_x':0.5, 'center_y':0.15}
                on_press: root.delete_selected_rows()
        
        BoxLayout:
            size_hint:1,None
            height:"1dp"
            pos_hint: {"center_x": .5, "center_y": .0}
            spacing:"20dp"
            height: dp(56) + self.spacing * (len(self.children) - 1)
           
        

        MDBottomNavigation:
            panel_color: 184/255, 190/255, 195/255,1
                    

            MDBottomNavigationItem:
                name: 'catalogue'
                text: 'Catalogue'
                icon: 'order-bool-descending-variant'
                icon_size: "36dp"

                MDLabel:
                    text:"1111"
                    halign: "center"
                    

                
            
            MDBottomNavigationItem:
                name: 'orders'
                text: 'Orders'
                icon: 'store-check'
                    
                MDLabel:
                    text: 'I programming of C++'
                    halign: 'center'
                        
                
            MDBottomNavigationItem:
                name: 'sales'
                text: 'Sales'
                icon: 'cash-check'
                icon_size: "36dp"

                BoxLayout:
                    orientation: 'vertical'
                    MDLabel:
                        text: '333'
                        halign: 'center'

                
                    
                        


<CatalogueScreen>
    name:"catalogue"     
   

<SalesScreen>
    name:"sales"     
    