ScreenManager:
    SalesGraph:
<SalesGraph>:
    name:"sales_graph"

    MDFloatLayout:
        id: graph

        MDIconButton:
            icon: 'arrow-left-circle'
            user_font_size: '80sp'
            pos_hint: {"center_x": .1, "center_y": .9}
            on_press: 
                root.manager.transition.direction = 'right'
                root.manager.current = 'orders'
        

        MDLabel:
            text:"sales"
            halign:"center"
            pos_hint:{'center_y':.8}
